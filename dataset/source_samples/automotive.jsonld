{
    "id": "catena-x-example-001",
    "odrl": {
      "@context": [
        "http://www.w3.org/ns/odrl.jsonld",
        {
          "cx-policy": "https://w3id.org/catenax/policy/"
        }
      ],
      "@type": "Set",
      "profile": "cx-policy:profile2405",
      "permission": [
        {
          "action": "use",
          "constraint": {
            "and": [
              {
                "leftOperand": "cx-policy:FrameworkAgreement",
                "operator": "eq",
                "rightOperand": "DataExchangeGovernance:1.0"
              },
              {
                "leftOperand": "cx-policy:ContractReference",
                "operator": "eq",
                "rightOperand": "x12345"
              },
              {
                "leftOperand": "cx-policy:UsagePurpose",
                "operator": "eq",
                "rightOperand": "cx.core.industrycore:1"
              }
            ]
          }
        }
      ]
    },
    "text": "This policy allows data use if the user complies with the Data Exchange Governance 1.0 agreement, references contract x12345, and the data is used for core industrial purposes as defined in the Catena-X profile."
  }
  